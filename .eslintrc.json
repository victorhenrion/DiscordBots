{
    "extends": [
        "universe/node"
    ],
    "ignorePatterns": [
        "**/node_modules/**"
    ],
    "root": true,
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false,
        "babelOptions": {
            "plugins": [
                "@babel/plugin-syntax-import-assertions"
            ]
        }
    }
}
